% ***************************************18*****************************
% *** Detectors
% **********************************************************************


det FluxT n
dr -2 void dm He3Top
det FluxB  n
dr -2 void dm He3Bot

det EnergyDep n
dr -80 void

det EnergyAssembly n
dr -80 void
dx 10.405 42.31 1
dy 28.405 52.31 10
dz -60.48375 60.48375 500

det HeatT n
dr -4 He3Top dm He3Top

det HeatB n
dr -4 He3Bot dm He3Bot

%Keff
det absorption
dr -2 void
%Keff 
det totalfission
dr -6 void

det fissionneutronprod
